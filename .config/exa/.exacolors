#!/data/data/com.termux/files/usr/bin/env bash
# replace bright aixterm colour codes w/ bold ANSI codes
export EXA_COLORS="${LS_COLORS//=9/=1;3}"

# Normal file
EXA_COLORS+="fi=0:"
# Directory
EXA_COLORS+="di=01;34:"
# Executable file
EXA_COLORS+="ex=38;5;002;1:"
# Named pipe
EXA_COLORS+="pi=01;31;44:"
# Socket
EXA_COLORS+="so=01;31;44:"
# Block device
EXA_COLORS+="bd=01;44:"
# Character device
EXA_COLORS+="cd=01;44:"
# Symlink
EXA_COLORS+="ln=01;38;5;123:"
# Arrow to broken symlink
EXA_COLORS+="or=30;48;5;212:"

## PERMISSIONS

# User +r bit
EXA_COLORS+="ur=1;33:"
# User +w bit
EXA_COLORS+="uw=1;31:"
# User +x bit (files)
EXA_COLORS+="ux=1;4;32:"
# User +x bit (file types)
EXA_COLORS+="ue=1;32:"
# Group +r bit
EXA_COLORS+="gr=33:"
# Group +w bit
EXA_COLORS+="gw=31:"
# Group +x bit
EXA_COLORS+="gx=32:"
# Others +r bit
EXA_COLORS+="tr=33:"
# Others +w bit
EXA_COLORS+="tw=31:"
# Others +x bit
EXA_COLORS+="tx=32:"
# Higher bits (files)
EXA_COLORS+="su=36:"
# Higher bits (other types)
EXA_COLORS+="sf=36:"
# Extended attribute marker
EXA_COLORS+="xa=1;34:"

## FILE SIZES

# Size numbers
# EXA_COLORS+="sn=36:"
# Size unit
# EXA_COLORS+="sb=1;36:"
# Major device ID
# EXA_COLORS+="df=1;36:"
# Minor device ID
# EXA_COLORS+="ds=36:"

## OWNERS AND GROUPS

# A user that's you
EXA_COLORS+="uu=1;33:"
# A user that's not
EXA_COLORS+="un=33:"
# A group with you in it
EXA_COLORS+="gu=1;33:"
# A group without you
EXA_COLORS+="gn=33:"

## HARD LINKS

# Number of links
EXA_COLORS+="lc=1;37:"
# A multi-link file
EXA_COLORS+="lm=01;38;5;212:"

## GIT

# New
EXA_COLORS+="ga=1;32:"
# Modified
EXA_COLORS+="gm=33:"
# Deleted
EXA_COLORS+="gd=1;31:"
# Renamed
EXA_COLORS+="gv=35:"
# Type change
EXA_COLORS+="gt=36:"

## DETAILS AND METADATA

# Punctuation
EXA_COLORS+="xx=0:"
# Timestamp
EXA_COLORS+="da=34:"
# File inode
EXA_COLORS+="in=35:"
# Number of blocks
EXA_COLORS+="bl=36:"
# Table header row
EXA_COLORS+="hd=4;37:"
# Symlink path
EXA_COLORS+="lp=1;33:"
# Control character
EXA_COLORS+="cc=1;31:"

## OVERLAYS

# Broken link path
EXA_COLORS+="bO=30;48;5;212:"

